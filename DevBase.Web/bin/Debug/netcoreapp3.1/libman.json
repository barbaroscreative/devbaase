{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/jquery/"
    },
    {
      "library": "twitter-bootstrap@4.5.3",
      "destination": "wwwroot/js/twitter-bootstrap/",
      "files": [
        "js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.2",
